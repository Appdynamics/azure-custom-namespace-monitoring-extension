<!--
aggregationType = AVERAGE | SUM | OBSERVATION
timeRollUpType = AVERAGE | SUM | CURRENT
clusterRollUpType = INDIVIDUAL | COLLECTIVE
-->
<!--
Set delta = true for metric to report per-min data
-->

<stats>
    <!--Add stat for each of the service-->
    <stat service="Virtual machine" resourceType="Microsoft.Compute">
        <metric attr="Percentage CPU" alias="Percentage CPU" timeSpan="PT1M" aggregationType="AVERAGE" timeRollUpType="AVERAGE" clusterRollUpType="COLLECTIVE"/>
        <metric attr="Network In Billable" alias="Network In Billable" timeSpan="PT1M" aggregationType="AVERAGE" timeRollUpType="AVERAGE" clusterRollUpType="COLLECTIVE"/>
        <metric attr="Network Out Billable" alias="Network Out Billable" timeSpan="PT1M" aggregationType="AVERAGE" timeRollUpType="AVERAGE" clusterRollUpType="COLLECTIVE"/>
        <metric attr="Disk Read Bytes" alias="Disk Read Bytes" timeSpan="PT1M" aggregationType="AVERAGE" timeRollUpType="AVERAGE" clusterRollUpType="COLLECTIVE"/>
        <metric attr="Disk Write Bytes" alias="Disk Write Bytes" timeSpan="PT1M" aggregationType="AVERAGE" timeRollUpType="AVERAGE" clusterRollUpType="COLLECTIVE"/>
        <metric attr="Disk Read Operations/Sec" alias="Disk Read Operations/Sec" timeSpan="PT1M" aggregationType="AVERAGE" timeRollUpType="AVERAGE" clusterRollUpType="COLLECTIVE"/>
        <metric attr="Disk Write Operations/Sec" alias="Disk Write Operations/Sec" timeSpan="PT1M" aggregationType="AVERAGE" timeRollUpType="AVERAGE" clusterRollUpType="COLLECTIVE"/>
        <metric attr="CPU Credits Remaining" alias="CPU Credits Remaining" timeSpan="PT1M" aggregationType="AVERAGE" timeRollUpType="AVERAGE" clusterRollUpType="COLLECTIVE"/>
    </stat>
</stats>